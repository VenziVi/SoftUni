@model List<Contact>
@{
    ViewData["Title"] = "Contacts";
}

<head>
    <title>Phonebook</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
</head>


<div class="row">
    <div class="col-md-4">
        <h3>Phonebook</h3>
        <table class="table">
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.Name
                        </td>
                        <td>
                            @item.Number
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


